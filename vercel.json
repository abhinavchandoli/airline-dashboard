        "source": "/airline/:airline*",
        "destination": "/index.html"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "routes": [
      {
        "src": "/airline/.*",
        "dest": "/index.html"
      }
    ]
  }